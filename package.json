{
  "name": "cloudflare-xpanel",
  "version": "1.0.0",
  "description": "VPN销售管理系统",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:api": "wrangler dev functions/api/[[route]].ts --env production",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "wrangler pages deploy dist",
    "deploy:api": "wrangler deploy functions/api/[[route]].ts",
    "db:generate": "wrangler d1 execute xpanel-db --env production --file=./database/schema.sql",
    "db:seed": "wrangler d1 execute xpanel-db --env production --file=./database/seed.sql",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.8.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "hono": "^4.9.8",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.294.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "wrangler": "^3.15.0"
  }
}
